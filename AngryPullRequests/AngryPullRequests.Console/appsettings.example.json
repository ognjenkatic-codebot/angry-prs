{
  "RepositoryConfiguration": {
    "Repository": "<REPO_NAME>",
    "Owner": "<REPO_OWNER>",
    "AccessToken": "<GITHUB_PERSONAL_ACCESS_TOKEN>"
  },
  "SlackConfiguration": {
    "AccessToken": "<APP_LEVEL_TOKEN>",
    "ApiToken": "<BOT_TOKEN>",
    "NotificationsChannel": "<SLACK_CHANNEL>"
  },
  "Scheduling": {
    "Hour": "<HOUR_OF_RUN>",
    "Minute": "<MINUTE_OF_RUN>"
  },
  "JiraConfiguration": {
    "IssueBaseUrl": "https://<JIRA_BASE_URL>/browse/",
    "IssueRegex": "<ISSUE_CAPTURE_GROUP>"
  },
  "PullRequestPreferences": {
    "NameRegex": ".*",
    "NameCaptureRegex": "^PROJ-[0-9]+ \\|(.*$)",
    "ReleaseTagRegex": "Q[0-9]+\\.[0-9]+\\.[0-9]+",
    "InProgressLabel": "in progress",
    "SmallPrChangeCount": 100,
    "LargePrChangeCount": 500,
    "OldPrAgeByDays": 10,
    "InactivePrAgeByDays": 3,
    "DeleteHeavyRatio": 0.2
  }
}
